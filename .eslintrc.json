{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:reommended",
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommeded"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
